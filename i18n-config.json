{
	"vendor": {
		"plugins/wp-seo-qtranslate-x": "1.0"
	},
	"admin-config": {
		"yoast_wpseo": {
			"pages": {
				"post.php": "",
				"post-new.php": "",
				"term.php": "",
				"edit-tags.php": "action=edit"
			},
			"forms": {
				"document": {
					"fields": {
						"yoast_wpseo_title": {
							"encode": "{"
						},
						"yoast_wpseo_metadesc": {
							"encode": "{"
						},
						"yoast_wpseo_focuskw": {
							"encode": "{"
						},
						"yoast_wpseo_metakeywords": {},
						"hidden_wpseo_title": {
							"encode": "{"
						},
						"hidden_wpseo_desc": {
							"encode": "{"
						},
						"wpseo_metakey": {
							"encode": "{"
						},
						"wpseo_canonical": {
							"encode": "{"
						},
						"img-alt": {
							"jquery": "img",
							"attrs": [
								"alt"
							],
							"encode": "display"
						}
					}
				}
			}
		},
		"wpseo_settings": {
			"pages": {
				"admin.php": "page=wpseo_titles"
			},
			"forms": {
				"wpseo-conf": {
					"fields": {
						"wpseo_title": {
							"jquery": "[id^='title']"
						},
						"wpseo_metadesc": {
							"jquery": "[id^='metadesc']"
						},
						"wpseo_bctitle": {
							"jquery": "[id^='bctitle']"
						}
					}
				}
			}
		},
		"wpseo_settings_advanced": {
			"pages": {
				"admin.php": "page=wpseo_advanced"
			},
			"forms": {
				"wpseo-conf": {
					"fields": {
						"breadcrumbs-home": {},
						"breadcrumbs-prefix": {},
						"breadcrumbs-archiveprefix": {},
						"breadcrumbs-searchprefix": {},
						"breadcrumbs-404crumb": {},
						"rssbefore": {},
						"rssafter": {}
					}
				}
			}
		}
	},
	"front-config": {
		"all-pages": {
			"filters": {
				"text": {
					"wpseo_title": "20",
					"wpseo_meta": "20",
					"wpseo_metadesc": "20",
					"wpseo_metakeywords": "20",
					"wpseo_replacements": "20"
				}
			}
		}
	}
}
